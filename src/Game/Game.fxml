<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#handleKeyPressed" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.GameController">
    <children>
      <Canvas fx:id="canvas" height="720.0" layoutX="-1.0" width="1280.0" />
        <Button fx:id="musicButton" layoutX="1184.0" layoutY="624.0" mnemonicParsing="false" onAction="#toggleMusic" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/music_on.png');" />
        <Button fx:id="soundButton" layoutX="1088.0" layoutY="624.0" mnemonicParsing="false" onAction="#toggleSound" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/sound_on.png');" />
        <Button fx:id="mainMenuButton" layoutX="32.0" layoutY="624.0" mnemonicParsing="false" onAction="#openMainMenu" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/home.png');" styleClass="button" />
        <Button fx:id="pauseButton" layoutX="1184.0" layoutY="14.0" mnemonicParsing="false" onAction="#pause" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/pause.png');" />
    </children>
</AnchorPane>
