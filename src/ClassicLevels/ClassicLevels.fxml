         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="516.0" layoutY="127.0" prefHeight="78.0" prefWidth="285.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Button fx:id="loadCustomButton" layoutX="755.0" layoutY="352.0" mnemonicParsing="false" onAction="#loadCustomLevel" prefHeight="47.0" prefWidth="153.0" styleClass="defaultButton" text="Load custom level" />
