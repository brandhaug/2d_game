<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-image: url('Resources/background/background.png');" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CreateLevel.CreateLevelController">
    <children>
        <Canvas fx:id="canvas" height="704.0" onMouseClicked="#mouseClicked" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed" width="1280.0" />
        <AnchorPane layoutY="704.0" prefHeight="80" prefWidth="1280" style="-fx-background-color: Gray;">
            <children>
                <Button fx:id="playerButton" layoutX="10.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseStartingPoint" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/player.png');" />
                <Button fx:id="tileButton" layoutX="100.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseTile" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/tile.png');" />
                <Button fx:id="coinButton" layoutX="192.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseCoin" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/coin.png');" />
                <Button fx:id="enemy1Button" layoutX="282.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseEnemy1" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/monster_A.png');" />
                <Button fx:id="enemy2Button" layoutX="375.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseEnemy2" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/monster_C.png');" />
                <Button fx:id="chestButton" layoutX="471.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseEndPoint" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/chest.png');" />
                <Button fx:id="eraseButton" layoutX="563.0" layoutY="4.0" mnemonicParsing="false" onAction="#chooseEraser" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/erase.png');" />
                <Button fx:id="stepBackwardButton" layoutX="753.0" layoutY="4.0" mnemonicParsing="false" onAction="#stepBackward" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/undo.png');" />
                <Button fx:id="stepForwardButton" layoutX="849.0" layoutY="4.0" mnemonicParsing="false" onAction="#stepForward" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/redo.png');" />
                <Button fx:id="loadButton" layoutX="1132.0" layoutY="9.0" mnemonicParsing="false" onAction="#openLoadFile" prefHeight="64.0" prefWidth="64.0" text="Load" />
                <Button fx:id="saveButton" layoutX="1206.0" layoutY="9.0" mnemonicParsing="false" onAction="#openSaveFile" prefHeight="64.0" prefWidth="64.0" text="Save" />
            <Button fx:id="helpButton" layoutX="996.0" layoutY="9.0" mnemonicParsing="false" onAction="#openHelp" prefHeight="64.0" prefWidth="64.0" text="Help" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
