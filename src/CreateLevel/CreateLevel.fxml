<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-image: url('Resources/background/background.png');" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CreateLevel.CreateLevelController">
   <children>
      <Canvas fx:id="canvas" height="640.0" width="1152.0" />
      <AnchorPane layoutY="720.0" prefHeight="80" prefWidth="1280" style="-fx-background-color: Black;">
          <children>
            <Button fx:id="tileButton" layoutX="20" mnemonicParsing="false" onAction="#chooseTile" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/tile.png');" />
            <Button fx:id="coinButton" layoutX="100" mnemonicParsing="false" onAction="#chooseCoin" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/coin.png');" />
            <Button fx:id="enemyButton" layoutX="180" mnemonicParsing="false" onAction="#chooseEnemy" prefHeight="64.0" prefWidth="64.0" style="-fx-graphic: url('Resources/buttons/mace.png');" />
            <Button fx:id="loadButton" layoutX="1200" mnemonicParsing="false" onAction="#openLoadFile" prefHeight="64.0" prefWidth="64.0" />
          </children>
      </AnchorPane>
   </children>
</AnchorPane>
